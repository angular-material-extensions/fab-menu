{
  "name": "fab-menu",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4901",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:docs": "npx compodoc -p projects/angular-material-extensions/fab-menu/tsconfig.lib.json --hideGenerator --disableCoverage -d dist/browser/doc/",
    "build:lib": "ng build @angular-material-extensions/fab-menu --prod",
    "build:lib:Watch": "ng build @angular-material-extensions/fab-menu --watch",
    "deploy": "ng deploy --base-href=/fab-menu/ --no-build",
    "generate:prerender": "cd dist && node prerender",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "postinstall": "ngcc",
    "serve:prerender": "cd dist/browser && http-server",
    "release": "release-it",
    "test:watch": "jest --watch",
    "compile:server_bak": "webpack --config webpack.server.config.js --progress --colors",
    "build:ssr_bak": "npm run build:client-and-server-bundles && npm run compile:server",
    "serve:ssr_bak": "node dist/server",
    "build:client-and-server-bundles_bak": "ng build --prod && ng run fab-menu:server:production --bundleDependencies all",
    "dev:ssr": "ng run fab-menu:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --prod && ng run fab-menu:server:production",
    "prerender": "ng run fab-menu:prerender",
    "prerender:ghpages": "ng run fab-menu:prerender_ghpages"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.2",
    "@angular/cdk": "^11.0.0",
    "@angular/common": "^11.0.2",
    "@angular/compiler": "^11.0.2",
    "@angular/core": "^11.0.2",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "^11.0.2",
    "@angular/material": "^11.0.0",
    "@angular/platform-browser": "^11.0.2",
    "@angular/platform-browser-dynamic": "^11.0.2",
    "@angular/platform-server": "^11.0.2",
    "@angular/router": "^11.0.2",
    "@nguniversal/express-engine": "^11.0.0",
    "angular-cli-ghpages": "^0.6.2",
    "angulartics2": "^10.0.0",
    "express": "^4.17.1",
    "ngx-markdown": "^10.1.1",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.2",
    "@angular-devkit/build-ng-packagr": "^0.1001.0",
    "@angular/cli": "^11.0.2",
    "@angular/compiler-cli": "^11.0.2",
    "@angular/language-service": "^11.0.2",
    "@compodoc/compodoc": "^1.1.11",
    "@nguniversal/builders": "^11.0.0",
    "@release-it/conventional-changelog": "^1.1.4",
    "@schematics/angular": "^9.1.12",
    "@schematics/update": "^0.900.7",
    "@types/express": "^4.17.8",
    "@types/jasmine": "^3.5.14",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.55",
    "codelyzer": "^5.2.2",
    "http-server": "^0.12.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "^3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^11.0.2",
    "protractor": "^7.0.0",
    "release-it": "^12.6.3",
    "ts-loader": "^6.2.2",
    "ts-node": "~8.6.2",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "webpack-cli": "^3.3.12"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "greenkeeper": {
    "ignore": [
      "@angular/animations",
      "@angular/core",
      "@angular/common",
      "@angular/compiler",
      "@angular/cdk",
      "@angular/material",
      "@angular/platform-server",
      "@angular/platform-browser",
      "@angular/platform-browser-dynamic",
      "@angular/compiler-cli",
      "webpack",
      "zone.js",
      "rxjs",
      "tslint",
      "typescript",
      "codelyzer",
      "@types/jasmine",
      "@types/jest",
      "@types/node"
    ]
  }
}
